<div class="calColorBox boxShadow" id="calColorBox">
	<div class="calColorHeader">
		<div id="closeColorBox" class="setCloseBtn radius40" onclick="closeColorBox();" title="Close"><i class="fa fa-close"></i></div>
		<label>Set color for this calendar</label>
	</div>
	<%
		@colors.each do |color|
	%>
	<div class="calColorItem" onclick="selectItem('<%=color.color%>','<%=color.colorId%>');" style="background-color: <%= color.color %>"></div>
	<%
		end
	%>
</div>
